<?xml version="1.0" encoding="utf-8"?>
<rotate
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="90"
    android:toDegrees="0"
    >

    <shape
        android:shape="line">

        <stroke
            android:color="#aaaaac"
            android:dashWidth="2px"
            android:dashGap="2px"
            android:width="2dp"/>
    </shape>

</rotate>
